{
  "name": "STORE_NAME",
  "version": "0.0.1",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && concurrently \"npm:sass\" \"npm:vlink\" \"npm:sass-react\"",
    "sass": "sass --watch --no-source-map styles/scss:styles/css --style compressed",
    "sass-react": "sass --watch --no-source-map react/styles/scss:react/styles/css --style compressed\""
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "global": "^4.4.0",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@vtex/danger": "^0.2.9",
    "@vtex/intl-equalizer": "^2.5.1",
    "@vtex/prettier-config": "^1.0.0",
    "eslint": "^8.14.0",
    "eslint-config-vtex": "^15.0.0",
    "eslint-config-vtex-react": "^9.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
